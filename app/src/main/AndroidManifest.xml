<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.tacademy.airbnb">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:exported="true" />
        <activity android:name=".DetailLoginActivity" />
        <activity android:name=".OptionActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".FindPasswordActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".MessageActivity" />
        <activity android:name=".TravelActivity" />
        <activity
            android:name=".HelpActivity"
            android:exported="true" />
        <activity android:name=".CountryActivity"></activity>
    </application>

</manifest>